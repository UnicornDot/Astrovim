{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "demo",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}",
      "protocol": "inspector",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
